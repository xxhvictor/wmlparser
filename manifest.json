{
   "author": "xiexuehui(xxhforest@gmail.com)",
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "wml.css" ],
      "exclude_globs": [  ],
      "include_globs": [ "*" ],
      "js": [ "wml.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "parsing wml page using html page.",
   "icons": {
      "128": "img/icon-128.png",
      "16": "img/icon-16.png",
      "19": "img/icon-19.png",
      "48": "img/icon-48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18",
   "name": "WML",
   "permissions": [ "http://*/*", "https://*/*", "webRequest", "tabs", "webRequestBlocking", "management" ],
   "version": "1.0"
}
